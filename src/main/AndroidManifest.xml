<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp.jd_dome">

    <uses-permission android:name="android.permission.VIBRATE" /> <!-- 震动权限 -->
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" /> <!-- 使用照相机权限 -->
    <uses-feature android:name="android.hardware.camera.autofocus" /> <!-- 自动聚焦权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".view.activity.QRApplication"
        android:allowBackup="true"
        android:icon="@drawable/jd_shop_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/jd_shop_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.LoginActivity" />
        <activity android:name=".view.activity.RegActivity" />
        <activity android:name=".view.activity.TitleActivity" />
        <activity android:name=".view.activity.ZXingActivity" />
        <activity android:name=".view.activity.MiaoShaActivity" />
        <activity android:name=".view.activity.DetailsActivity" />
        <activity android:name=".view.activity.ListActivity"></activity>
    </application>

</manifest>